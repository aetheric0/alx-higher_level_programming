#!/usr/bin/python3
def safe_print_list_integers(my_list=[], x=0):
    try:
        count = 0
        for i in range(my_list):
                if isinstance(i, int):
                    print("{:d}".format(item), end="")
                    count += 1
                else:
                    continue
        print()
    except IndexError:
        raise IndexError("list index out of range")

    return count
